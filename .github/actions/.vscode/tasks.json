{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "typescript",
			"tsconfig": "get_cache/tsconfig.json",
			"problemMatcher": [
				"$tsc"
			],
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"label": "tsc: build - get_cache/tsconfig.json"
		},
		{
			"label": "Pack to distribute get_cache action",
			"type": "shell",
			"command": "ncc build get_cache/build/main.js -o get_cache/dist",
			"dependsOn": "tsc: build - get_cache/tsconfig.json",
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"presentation": {
			"reveal": "always",
			"panel": "shared"
			}
		}
	]
}